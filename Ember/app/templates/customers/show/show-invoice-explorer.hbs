<table class="table table-hover table-bordered table-sm listing">
  <thead>
    <tr>
      <th>Customer</th>
      <th>key</th>
      <th>Invoice Date</th>
      <th>Amount</th>
    </tr>
  </thead>
  <tbody class='mybody'>
    {{#each model as |invoiceRow| }}
      <tr class="myrow">
        <td>
          {{#link-to "invoices.show.show-detail" invoiceRow._key class="customerName"}}
            {{invoiceRow.customerName}}
          {{/link-to}}
        </td>
        <td class="key">{{invoiceRow._key}}</td>
        <td class="formula">{{invoiceRow.invoiceDate}}</td>
        <td class="type">{{invoiceRow.totAmount}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

{{outlet}}
